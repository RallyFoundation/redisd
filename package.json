{
  "author": "Henri Bouvier",
  "name": "redisd",
  "description": "A redis REST API",
  "version": "0.0.9",
  "keywords": [
    "redis",
    "rest",
    "redisd"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hbouvier/redisd.git"
  },
  "dependencies": {
    "node-options": "0.0.3",
    "redis": "0.10.3",
    "tomahawk": "0.0.17",
    "winston": "0.7.2"
  },
  "engines": {
    "node": ">= 0.10.0 < 0.11.0"
  },
  "main": "./lib/redis.js",
  "bin": {
    "redisd": "./bin/redisd"
  },
  "scripts": {
    "start": "bin/redisd"
  },
  "configuration": {
    "level": "info",
    "context": "/redis",
    "redis_urls": "127.0.0.1:6379"
  }
}
